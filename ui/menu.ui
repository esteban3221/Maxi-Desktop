              <object class="GtkBox">
                <child>
                  <object class="GtkImage">
                    <property name="icon-name">system-log-out-symbolic</property>
                  </object>
                </child>
                <property name="spacing">10</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="yes">Cerrar Sesion</property>
                  </object>
                </child>
              </object>
            </property>
            <style>
              <class name="flat"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkSeparator"></object>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="scrolled_window">
            <property name="propagate-natural-width">true</property>
            <property name="vexpand">true</property>
            <child>
              <object class="GtkListBox" id="listBoxMenu">
                <property name="valign">1</property>
                <property name="activate-on-single-click">true</property>
                <property name="selection-mode">1</property>
                <child>
                  <object class="GtkBox">
                    <property name="spacing">10</property>
                    <child>
                      <object class="GtkImage">
                        <property name="icon-name">money-symbolic</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
